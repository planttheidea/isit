{
  "author": "planttheidea",
  "bugs": {
    "url": "https://github.com/planttheidea/isit.js/issues"
  },
  "description": "micro check library",
  "devDependencies": {
    "babel": "6.5.2",
    "babel-cli": "6.10.1",
    "babel-eslint": "6.1.0",
    "babel-loader": "6.2.4",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-polyfill": "6.9.1",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-stage-0": "6.5.0",
    "babelify": "7.3.0",
    "browserify": "13.0.1",
    "ejs": "2.4.2",
    "eslint": "3.0.1",
    "eslint-friendly-formatter": "2.0.5",
    "eslint-loader": "1.4.0",
    "eslint-plugin-babel": "3.3.0",
    "fs": "0.0.2",
    "in-publish": "^2.0.0",
    "jquery": "3.0.0",
    "path": "0.12.7",
    "tape": "4.6.0",
    "tape-run": "2.1.4",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1"
  },
  "homepage": "https://github.com/planttheidea/isit.js",
  "keywords": [
    "check",
    "is",
    "is.js",
    "isit",
    "isitjs",
    "test",
    "validate"
  ],
  "license": "MIT",
  "main": "lib",
  "name": "isitjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/planttheidea/isit.js.git"
  },
  "scripts": {
    "build": "NODE_ENV=development ./node_modules/webpack/bin/webpack.js --progress",
    "build-minified": "NODE_ENV=production ./node_modules/webpack/bin/webpack.js --progress --config=webpack.config.minified.js",
    "compile-for-publish": "npm run test && npm run transpile && npm run dist",
    "dev": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --progress --config=webpack.config.dev.js",
    "dist": "npm run build && npm run build-minified",
    "lint": "NODE_ENV=test eslint src",
    "prepublish": "in-publish && npm run compile-for-publish || in-install",
    "test": "NODE_ENV=test browserify -t babelify test/index.js | tape-run",
    "transpile": "./node_modules/babel-cli/bin/babel.js -d lib/ src/"
  },
  "version": "2.3.1"
}
