{
  "author": "planttheidea",
  "ava": {
    "babel": "inherit",
    "concurrency": 5,
    "failFast": true,
    "files": [
      "test/*.js",
      "test/_internal/*.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill",
      "./test/helpers/setup-browser-env.js"
    ],
    "verbose": true
  },
  "bugs": {
    "url": "https://github.com/planttheidea/check-if/issues"
  },
  "description": "micro check library",
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-latest": "6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.23.0",
    "browser-env": "^2.0.21",
    "eslint": "^3.16.0",
    "eslint-config-rapid7": "^2.7.0",
    "eslint-friendly-formatter": "^2.0.7",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-babel": "^4.0.1",
    "html-webpack-plugin": "^2.28.0",
    "in-publish": "^2.0.0",
    "jquery": "^3.1.1",
    "moment": "^2.17.1",
    "nyc": "^10.1.2",
    "optimize-js-plugin": "^0.0.4",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "rimraf": "^2.6.0",
    "sinon": "^1.17.7",
    "webpack": "^2.2.1",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-server": "^2.4.1"
  },
  "homepage": "https://github.com/planttheidea/check-if",
  "keywords": [
    "check",
    "is",
    "if",
    "test",
    "validate"
  ],
  "license": "MIT",
  "main": "lib",
  "name": "check-if",
  "repository": {
    "type": "git",
    "url": "https://github.com/planttheidea/check-if.git"
  },
  "scripts": {
    "build": "NODE_ENV=development webpack --progress --profile --colors",
    "build:minified": "NODE_ENV=production webpack --progress --profile --colors --config=webpack.config.minified.js",
    "clean": "rimraf dist",
    "clean:files": "rimraf _internal index.js arithmetic.js array.js environment.js object.js presence.js regexp.js string.js time.js types.js isit*.js",
    "dev": "NODE_ENV=development webpack-dev-server --progress --profile --config=webpack.config.dev.js",
    "dev:dashboard": "NODE_ENV=development webpack-dashboard -p 3210 -- webpack-dev-server --profile --config=webpack.config.dev.js",
    "dev:dashboard:minimal": "NODE_ENV=development webpack-dashboard -m -p 3210 -- webpack-dev-server --profile --config=webpack.config.dev.js",
    "dist": "npm run build && npm run build:minified",
    "lint": "NODE_ENV=test eslint src",
    "prepublish": "in-publish && npm run prepublish:compile || echo",
    "prepublish:compile": "npm run lint && npm run test:coverage && npm run clean && npm run transpile && npm run dist",
    "profile": "NODE_ENV=production webpack --profile --progress --colors --config=webpack.config.minified.js --json > webpack.stats.config",
    "start": "npm run dev:dashboard:minimal",
    "test": "NODE_PATH=. NODE_ENV=test ava --no-cache",
    "test:coverage": "nyc npm test",
    "test:watch": "NODE_PATH=. NODE_ENV=test ava --watch",
    "transpile": "babel src --out-dir ."
  },
  "version": "1.0.0"
}
